                //How Do I Use jQuery to tell the button what to do when it is clicked--//


                $("#btnNumber").click(function () {

                    //How Do I Use jQuery to tell the button what to do when it is clicked--//
                    var num1 = parseInt($("#num1").val());
                    var num2 = parseInt($("#num2").val());
                    var num3 = parseInt($("#num3").val());
                    var num4 = parseInt($("#num4").val());
                    var num5 = parseInt($("#num5").val());


                    //$("#resultHdr").show();

                    var sum = (num1 + num2 + num3 + num4 + num5);
                    //Calculate the Product
                    var product = (num1 * num2 * num3 * num4 * num5);
                    //Calculate the Aaverage
                    var average = sum / 5;
                    //Determine the Largest
                    var max = Math.max(num1, num2, num3, num4, num5);
                    //Determine the Smallest
                    var min = Math.min(num1, num2, num3, num4, num5);

                    $("#sum").text("the sum is equal to " + sum);
                    $("#product").text("the product is equal to " + product);
                    $("#average").text("the average is equal to " + average);
                    $("#max").text("the maximum number is equal to " + max);
                    $("#min").text("the minimum number is equal to " + min);

                });


                $("#btnNumber2").click(function () {
                    $("#num1,#num2,#num3,#num4,#num5").val("");

                    $("#sum,#product,#average,#max,#min").html("");
                    //$("#resultHdr").hide();
                });

               //factorial modal section

                $("#factcode").hide();
                $("#showcd2").click(function () {
                    $("#factcode").toggle();
                });

                 //attempt to do the factorial equation

                $("#facbtn").click(function () {
                    var n1 = parseInt($("#n1").val());
                    var factorial = 1;
                    while (n1 > 1) {
                        factorial *= n1
                        n1--;
                    }
                    $("#facresults").text("Results for your Number is: " + factorial)
                    $("#fachdr").show();
                 });

                $("#facclear").click(function () {


                    $("#n1").val("");
                    $("#facresults").text("");
                    $("#fachdr").hide("");


                });

                //end of factorial section


                //fizz buzz section
                $("#fbhdr").hide();
                $("#fbcode").hide();
                $("#showcd3").click(function () {
                    $("#fbcode").toggle();
                });

                $("#fbclear").click(function () {
                    $("#fbresults").html("");
