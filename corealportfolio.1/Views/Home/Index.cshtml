                //reverse array
                $("#Palenter").click(function () {
                    $("#palhdr").show();

                    //the jquery is being used as a var named pally
                    var pally = $("#pal1").val();
                    //
                    var output = "";
                    var arr = pally.split("");
                    var new_arr = arr.reverse();
                    var rewrd = new_arr.join("")
                    if (pally == rewrd) {
                        output += "it works"
                    }
                    else {
                        output += "not the same"
                    }
                    $("#palresults").html(output)
                });

                $("#palclr").click(function () {
                    $("#pal1").val("");
                    $("#palresults").html("");
                    $("#palhdr").hide();
                });











