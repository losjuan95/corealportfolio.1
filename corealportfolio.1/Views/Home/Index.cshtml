                    $("#buzz").val("");

                });

               //this code is what i will use to take off the trailing comma as soon as i have time to figure it out
               //var last = myOutput[myOutput.length -1]


                $("#fbbutton").click(function () {
                    //Collect user data
                    var num1 = parseInt($("#fizz").val());
                    var num2 = parseInt($("#buzz").val());
                    var myOutput = "";
                                    

                    //Write a loop that goes from 1 to 100
                    for (var loop = 1; loop <= 100; loop++) {
                        //Record my modulus operation values

                        //loop counter = 15
                        var fizz = loop % num1;
                        var buzz = loop % num2;

                        //Create a variable that will hold a string
                    

                        //Check for Fizz
                        if (fizz == 0 && buzz == 0) {
                            myOutput += "<span class='fizzbuzz'>Fizz-Buzz</span>, ";
                        }
                        else if (fizz == 0) {
                            myOutput += "<span class='fizz'>Fizz</span>, ";
                        }
                        else if (buzz == 0) {
                            myOutput += "<span class='buzz'>Buzz</span>, ";
                        }
                        else {
                            myOutput += loop + ", ";
                        }
                        
                    }


                    //Write out results
                    //myOutput.substring(str.length , str.length);
                    $("#fbresults").html(myOutput);



                });

                //palindrome excercise
                $("#palrsesults").hide();
